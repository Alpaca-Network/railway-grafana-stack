                "enableLogDetails": true,
                "showCommonLabels": false,
                "showLabels": false,
                "showTime": true,
                "sortOrder": "Descending",
                "wrapLogMessage": false
            },
            "title": "Recent System Errors (Loki)",
            "type": "logs",
            "targets": [
                {
                    "datasource": {
                        "type": "loki",
                        "uid": "grafana_loki"
                    },
                    "expr": "{env=\"production\"} |= \"error\" or |= \"exception\"",
                    "refId": "A"
                }
            ]
        },
        {
