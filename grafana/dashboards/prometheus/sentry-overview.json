            "title": "ðŸ—„ï¸ Database Performance (Prometheus/Tempo)",
            "type": "row"
        },
        {
            "datasource": {
                "type": "prometheus",
                "uid": "grafana_prometheus"
            },
            "fieldConfig": {
                "defaults": {
                    "custom": {
                        "axisPlacement": "auto",
                        "drawStyle": "line",
                        "fillOpacity": 10,
                        "lineInterpolation": "smooth",
                        "lineWidth": 2,
                        "showPoints": "never"
                    },
                    "unit": "s"
                }
            },
            "gridPos": {
                "h": 8,
                "w": 12,
                "x": 0,
                "y": 6
            },
            "id": 11,
            "title": "DB Query Duration (Average by Table)",
            "type": "timeseries",
            "targets": [
                {
                    "datasource": {
                        "type": "prometheus",
                        "uid": "grafana_prometheus"
                    },
                    "expr": "rate(database_query_duration_seconds_sum[5m]) / rate(database_query_duration_seconds_count[5m])",
                    "legendFormat": "{{table}}",
                    "refId": "A"
                }
            ]
        },
        {
            "datasource": {
                "type": "tempo",
                "uid": "grafana_tempo"
            },
            "gridPos": {
                "h": 8,
                "w": 12,
                "x": 12,
                "y": 6
            },
            "id": 12,
            "title": "Slow DB Transactions (Tempo Traces)",
            "type": "table",
            "targets": [
                {
                    "datasource": {
                        "type": "tempo",
                        "uid": "grafana_tempo"
                    },
                    "query": "{} | duration > 500ms",
                    "refId": "A"
                }
            ]
        },
        {
            "collapsed": false,
            "gridPos": {
                "h": 1,
                "w": 24,
                "x": 0,
                "y": 14
            },
            "id": 20,
            "panels": [],
            "title": "ðŸš€ Cache & API Performance",
            "type": "row"
        },
        {
            "datasource": {
                "type": "prometheus",
                "uid": "grafana_prometheus"
            },
            "fieldConfig": {
                "defaults": {
                    "color": {
                        "mode": "palette-classic"
                    },
                    "unit": "s"
                }
            },
            "gridPos": {
                "h": 8,
                "w": 12,
                "x": 0,
                "y": 15
            },
            "id": 21,
            "title": "Model Inference Latency (p95)",
            "type": "timeseries",
            "targets": [
                {
                    "datasource": {
                        "type": "prometheus",
                        "uid": "grafana_prometheus"
                    },
                    "expr": "histogram_quantile(0.95, sum by (le, provider) (rate(model_inference_duration_seconds_bucket[5m])))",
                    "legendFormat": "{{provider}}",
                    "refId": "A"
                }
            ]
        },
        {
            "datasource": {
                "type": "prometheus",
                "uid": "grafana_prometheus"
            },
            "fieldConfig": {
                "defaults": {
                    "mappings": [],
                    "thresholds": {
                        "mode": "absolute",
                        "steps": [
                            {
                                "color": "red",
                                "value": null
                            },
                            {
                                "color": "green",
                                "value": 80
                            }
                        ]
                    },
                    "unit": "percent"
                }
            },
            "gridPos": {
                "h": 8,
                "w": 12,
                "x": 12,
                "y": 15
            },
            "id": 22,
            "title": "Application Cache Hit Rate",
            "type": "gauge",
            "targets": [
                {
                    "datasource": {
                        "type": "prometheus",
                        "uid": "grafana_prometheus"
                    },
                    "expr": "sum(rate(cache_hits_total[5m])) / (sum(rate(cache_hits_total[5m])) + sum(rate(cache_misses_total[5m]))) * 100",
                    "refId": "A"
                }
            ]
        }
    ],
