                },
                "textMode": "auto"
            },
            "title": "Backend Status",
            "type": "stat",
            "targets": [
                {
                    "datasource": {
                        "type": "prometheus",
                        "uid": "grafana_prometheus"
                    },
                    "expr": "up{job=\"gatewayz_production\"}",
                    "refId": "A"
                }
            ]
        },
        {
            "datasource": {
